# Source Prezto.
if [[ -s "${ZDOTDIR:-$HOME}/.zprezto/init.zsh" ]]; then
  source "${ZDOTDIR:-$HOME}/.zprezto/init.zsh"
fi

export PATH=$HOME/.local/bin:$PATH

alias Suy='pacman -Suy'
type nvim > /dev/null && alias vim=nvim
